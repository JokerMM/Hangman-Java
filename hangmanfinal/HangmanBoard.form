    private void jMenuItem2ActionPerformed(java.awt.event.ActionEvent evt) {                                           
        // TODO add your handling code here:
        String result = JOptionPane.showInputDialog(null, "Enter printer name:");

        if ((result != null) && (result.length() > 0)) this.player_name = result;
        refresh(); 
    }                                          

    private void jMenuItem4ActionPerformed(java.awt.event.ActionEvent evt) {                                           
                      int prompt = JOptionPane.showConfirmDialog(null,
                        "Are you sure you want to quit?",
                        "Quit?", 
                        JOptionPane.YES_NO_OPTION);
                
                if (prompt == JOptionPane.YES_OPTION)
                    System.exit(0); 
    }                                          

    private void labelClick(java.awt.event.MouseEvent evt) {                            
        
        JLabel label = (JLabel)evt.getSource();
        char test = label.getText().charAt(0);
        if(label.isEnabled())
        {
            label.setEnabled(false);
            //word.checkletter(test);
            if(word.checkletter(test)) playerScore.addpoint();
            refresh();
        }
        System.out.println(test);

    }                           

    private void jMenuItem1ActionPerformed(java.awt.event.ActionEvent evt) {                                           
        word.newgame();
        playerScore.resetscore();
        playerScore.checktopscore(player_name ,playerScore.getScore());

        //must put the check top score here

        Refreshletter();
        refresh();        // TODO add your handling code here:
    }                                          

