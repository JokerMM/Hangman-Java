    readtopscr topscr = new readtopscr();
    manageword word = new manageword();
    ScoreManage playerScore = new ScoreManage();
    
    private String player_name;
    public HangmanBoard() {
        this("Guest");
    }
    
    public HangmanBoard(String pl_name) {
        initComponents();
        topscores.setText(topscr.printtopscores());
        this.player_name = pl_name;
        PlayerName.setText(player_name);
        Message.setText("Attempts left :   " + (word.GuessNumber-1) + "");
        refresh();
        addCloseWindowListener();
        //createletterpanel();

    }
    
    private void Refreshletter(){
                
    for (Component jc : jPanel4.getComponents()) {
        if ( jc instanceof JLabel ) {
            for (char charrr  = 'a' ; charrr <= 'z' ; charrr++){
                if (charrr == ((JLabel) jc).getText().charAt(0))
                        {
                           jc.setEnabled(true);
                        }
            }
        }
    }
        
    }
    
    private void refresh(){
        PlayerScore.setText(playerScore.toString());
        wordtoguess.setText(word.toString());
        PlayerName.setText(player_name);
        topscores.setText(topscr.printtopscores());
        Message.setText("Attempts left :   " + (word.GuessNumber-1) + "");
        if(word.checkwordfinish())
        {
            JOptionPane.showMessageDialog(null,
                "<html>You guessed the word<br> your score is " + playerScore.toString(),
                "YOU WON", 
                JOptionPane.DEFAULT_OPTION);
            
            word.newgame();
            Refreshletter();
            refresh();

        }
        hangman_photo_changer();
    }
    
    private void hangman_photo_changer(){
        
        String imagePath = "src/images/h" + word.GuessNumber + ".png";
        ImageIcon img ;
        img = new ImageIcon(imagePath);
        img.getImage().flush();
        if (word.GuessNumber == 1 ){

            hangmanphoto.setIcon(img);
            JOptionPane.showMessageDialog(null,
                        "<html>You Lost the Game. <br> your score is " + playerScore.toString() + "<br>The word is: <span style=\"color: red;\">" + word.getWord()+"</span>",
                        "YOU LOST", 
                        JOptionPane.DEFAULT_OPTION);
            word.newgame();
            playerScore.checktopscore(player_name ,playerScore.getScore());
            playerScore.resetscore();
            //must put the check top score here
            
            Refreshletter();
            refresh();
        }
        else{
            hangmanphoto.setIcon(img);
        }
    }
    
        private void addCloseWindowListener()
    {
        // NOTE: Must be DO_NOTHING_ON_CLOSE for prompt to function correctly
        setDefaultCloseOperation(JFrame.DO_NOTHING_ON_CLOSE);
        
        addWindowListener(new WindowAdapter()
        {
            @Override
            public void windowClosing(WindowEvent we)
            {
                int prompt = JOptionPane.showConfirmDialog(null,
                        "Are you sure you want to quit?",
                        "Quit?", 
                        JOptionPane.YES_NO_OPTION);
                
                if (prompt == JOptionPane.YES_OPTION)
                    System.exit(0);
            }
        });
    }

    

